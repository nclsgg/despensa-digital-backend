# Configurações do servidor
PORT=3030
GIN_MODE=debug

# Banco de dados
DATABASE_URL=postgres://user:password@localhost:5432/despensa_digital?sslmode=disable

# JWT
JWT_SECRET=seu_jwt_secret_super_seguro_aqui
JWT_EXPIRATION=1h
JWT_ISSUER=despensa-digital
JWT_AUDIENCE=despensa-digital-users

# Redis
REDIS_URL=redis://localhost:6379
REDIS_USERNAME=
REDIS_PASSWORD=
REDIS_DB=0

# CORS
CORS_ORIGIN=http://localhost:3000

# OAuth Google - Obtenha em https://console.cloud.google.com/
GOOGLE_CLIENT_ID=seu_google_client_id_aqui
GOOGLE_CLIENT_SECRET=seu_google_client_secret_aqui
GOOGLE_CALLBACK_URL=http://localhost:3030/auth/oauth/google/callback

# Session Secret para OAuth (gere uma string aleatória segura)
SESSION_SECRET=sua_session_secret_super_segura_aqui_com_32_caracteres_ou_mais

# Frontend URL
FRONTEND_URL=http://localhost:3000
